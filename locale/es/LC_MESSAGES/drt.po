#: /usr/lib/python2.7/argparse.py:294 /usr/lib/python3.6/argparse.py:304
msgid "usage: "
msgstr "Uso: "

#: /usr/lib/python2.7/argparse.py:1601 /usr/lib/python3.6/argparse.py:1646
msgid "positional arguments"
msgstr "parámetros posicionales"

#: /usr/lib/python2.7/argparse.py:1602 /usr/lib/python3.6/argparse.py:1647
msgid "optional arguments"
msgstr "parámetros opcionales"

#: /usr/lib/python2.7/argparse.py:1617 /usr/lib/python3.6/argparse.py:1662
msgid "show this help message and exit"
msgstr "mostrar esta ayuda y terminar"

#: /usr/lib/python2.7/argparse.py:1623 drt.py:135
msgid "show program's version number and exit"
msgstr "mostrar la versión del programa y terminar"

#: /usr/lib/python2.7/argparse.py:1707 /usr/lib/python3.6/argparse.py:1745
msgid "unrecognized arguments: %s"
msgstr "parámetros desconocidos: %s"

#: /usr/lib/python2.7/argparse.py:1850 /usr/lib/python2.7/argparse.py:1864
#: /usr/lib/python3.6/argparse.py:1888 /usr/lib/python3.6/argparse.py:1902
msgid "ignored explicit argument %r"
msgstr "ignorado parámetro explícito %r"

#: /usr/lib/python2.7/argparse.py:1954
msgid "too few arguments"
msgstr "faltan parámetros"

#: /usr/lib/python2.7/argparse.py:2032 /usr/lib/python3.6/argparse.py:2067
msgid "expected one argument"
msgstr "se esperaba un parámetro"

#: /usr/lib/python2.7/argparse.py:2275
msgid "invalid %s value: %r"
msgstr "valor de tipo %s inválido: %r"

#: /usr/lib/python2.7/argparse.py:2378
msgid "%s: error: %s\n"
msgstr "%s: error: %s\n"

#: /usr/lib/python3.6/argparse.py:2009
msgid "the following arguments are required: %s"
msgstr "se necesitan los siguientes parámetros: %s"

#: /usr/lib/python3.6/argparse.py:2315
msgid "invalid %(type)s value: %(value)r"
msgstr "valor de tipo %(type)s inválido: %(value)r"

#: /usr/lib/python3.6/argparse.py:2402
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: error: %(message)s\n"

#: drt.py:105
msgid "With maximum abbreviation length %(maxAbrev)d, length of texts after compression: %(longDespues)d."
msgstr "Con longitud máxima de las abreviaturas %(maxAbrev)d, longitud de cadenas tras abreviar: %(longDespues)d."

#: drt.py:140
msgid "disable compatibility with original interpreters"
msgstr "deshabilitar compatibilidad con intérpretes originales"

#: drt.py:141
msgid "MIN_LENGTH"
msgstr "LONG_MIN"

#: drt.py:141
msgid "minimum abbreviation length (default: %(default)d)"
msgstr "longitud mínima de abreviaturas (por defecto: %(default)d)"

#: drt.py:142
msgid "MAX_LENGTH"
msgstr "LONG_MAX"

#: drt.py:142
msgid "maximum abbreviation length (default: %(default)d)"
msgstr "longitud máxima de abreviaturas (por defecto: %(default)d)"

#: drt.py:143
msgid "SUPERSET_LIMIT"
msgstr "LIMITE"

#: drt.py:143
msgid "limit for the superset search heuristic (default: %(default)d)"
msgstr "límite para heurística de búsqueda de superconjuntos (por defecto: %(default)d)"

#: drt.py:144
msgid "show additional information"
msgstr "mostrar información adicional"

#: drt.py:146
msgid "input.json"
msgstr "entrada.json"

#: drt.py:146
msgid "input filename, JSON database generated by DRC frontend"
msgstr "nombre del fichero de entrada, base de datos JSON generada por el frontend de DRC"

#: drt.py:147
msgid "output.tok"
msgstr "salida.tok"

#: drt.py:147
msgid "output filename, for the best tokens found"
msgstr "nombre del fichero de salida, para las mejores abreviaturas encontradas"

#: drt.py:167
msgid "Length of texts without compression (excluding objects):"
msgstr "Longitud de los textos sin abreviar (excluyendo objetos):"

#: drt.py:184
msgid "bytes saved from text compression"
msgstr "bytes ahorrados por abreviación de textos"

#: drt.py:187
msgid "The best combination of abbreviations was found with maximum abbreviation length"
msgstr "La mejor combinación de abreviaturas se encontró con longitud máxima de abreviatura"

#: drt.py:188
msgid "abbreviations in total, which are:"
msgstr "abreviaturas en total, que son:"

#: drt.py:215
msgid "Error converting abbreviation %(abreviatura)s: it has %(cuenta)d characters that exceed code 127, but aren't on daad_a_chr"
msgstr "Error al convertir la abreviatura %(abreviatura)s: tiene %(cuenta)d caracteres que exceden el código 127, pero no salen en daad_a_chr"

#: drt.py:228
msgid "Abbreviations stored in:"
msgstr "Abreviaturas guardadas en:"
